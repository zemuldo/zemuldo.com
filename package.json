    "lint": "eslint . --fix",
    "dev": "NODE_ENV=development node server.js",
    "build": "./setup/version_info.sh && NODE_ENV=production next build",
    "start": "NODE_ENV=production node server.js",
    "prod": "NODE_ENV=production pm2 start  server.js --name Zemuldo.COM",
