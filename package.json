    "analyze": "BUNDLE_ANALYZE=true next build",
    "storybook": "start-storybook -p 9001 -s ./ -c .storybook",
    "build-storybook": "build-storybook -c .storybook -o .stories --quiet",
    "test": "NODE_ENV=test GITHUB_SHA=94d077c24 VERSION=0.0.1 BRANCH=develop jest --u",
    "test:watch": "NODE_ENV=test jest --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
